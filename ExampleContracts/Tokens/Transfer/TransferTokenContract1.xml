	<humanReadableText xml:lang="da">
		<section>
			<header>
				<text>Overførsel af token</text>
			</header>
			<body>
				<paragraph>
					<text>Denne kontrakt overfører et token fra en ejer til en anden gennem en monetær transkation.</text>
				</paragraph>
				<section>
					<header>
						<text>Betaling</text>
					</header>
					<body>
						<paragraph>
							<text>Når </text>
							<italic>
								<text>Sælger</text>
							</italic>
							<text> og </text>
							<italic>
								<text>Køber</text>
							</italic>
							<text> har underskrevet kontrakten, og </text>
							<italic>
								<text>Trust Provider</text>
							</italic>
							<text> har godkendt indholdet, verificeret underskrifterne og underskrevet kontrakten, sendes beløbet på </text>
							<parameter name="Value" />
							<text> </text>
							<parameter name="Currency" />
							<text> fra </text>
							<italic>
								<text>Køber</text>
							</italic>
							<text> til </text>
							<italic>
								<text>Sælger</text>
							</italic>
							<text>, minus en provision på </text>
							<parameter name="CommissionPercent" />
							<text> % betalt til </text>
							<italic>
								<text>Trust Provider</text>
							</italic>
							<text>.</text>
						</paragraph>
					</body>
				</section>
				<section>
					<header>
						<text>Overdragelse af ejerskab</text>
					</header>
					<body>
						<paragraph>
							<text>Når betalingerne er realiseret, overføres ejerskabet af tokenet fra </text>
							<italic>
								<text>Sælger</text>
							</italic>
							<text> (eller </text>
							<italic>
								<text>tidligere ejer</text>
							</italic>
							<text>) til </text>
							<italic>
								<text>Køber</text>
							</italic>
							<text> (som bliver den nye ejer).</text>
						</paragraph>
					</body>
				</section>
			</body>
		</section>
	</humanReadableText>
	<humanReadableText xml:lang="de">
		<section>
			<header>
				<text>Token-Übertragung</text>
			</header>
			<body>
				<paragraph>
					<text>Dieser Vertrag überträgt einen Token von einem Eigentümer auf einen anderen durch eine Geldtranskation.</text>
				</paragraph>
				<section>
					<header>
						<text>Zahlung</text>
					</header>
					<body>
						<paragraph>
							<text>Sobald der </text>
							<italic>
								<text>Verkäufer</text>
							</italic>
							<text> und der </text>
							<italic>
								<text>Käufer</text>
							</italic>
							<text> den Vertrag unterzeichnet haben und der </text>
							<italic>
								<text>Trust Provider</text>
							</italic>
