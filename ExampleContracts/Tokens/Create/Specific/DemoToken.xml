	<humanReadableText xml:lang="da">
		<section>
			<header>
				<text>Opret demo-token</text>
			</header>
			<body>
				<paragraph>
					<text>Denne kontrakt opretter et simpelt demo-token.</text>
				</paragraph>
				<section>
					<header>
						<text>Indledende værdi</text>
					</header>
					<body>
						<paragraph>
							<text>Når tokenet oprettes, indstilles tokenet til den værdi, </text>
							<parameter name="Value" />
							<text> </text>
							<parameter name="Currency" />
							<text>. Tillidsudbyderen tager en provision på </text>
							<parameter name="CommissionPercent" />
							<text> % af denne værdi, når tokenet oprettes.</text>
						</paragraph>
					</body>
				</section>
				<section>
					<header>
						<text>Varighed</text>
					</header>
					<body>
						<paragraph>
							<text>Kontraktens løbetid er fastsat til to måneder. Tokenets udløbsdato er indstillet til </text>
							<parameter name="Expires" />
						</paragraph>
					</body>
				</section>
				<section>
					<header>
						<text>Privilegier</text>
					</header>
					<body>
						<paragraph>
							<text>Følgende tilladelser er kodet i tokenet:</text>
						</paragraph>
						<bulletItems>
							<item>
								<text>Skaberen kan ødelægge tokenet? </text>
								<parameter name="CreatorCanDestroy" />
							</item>
							<item>
								<text>Ejeren kan ødelægge tokenet? </text>
								<parameter name="OwnerCanDestroyIndividual" />
							</item>
						</bulletItems>
					</body>
				</section>
				<section>
					<header>
						<text>Handel</text>
					</header>
					<body>
						<paragraph>
							<text>Tokens kan handles frit med andre brugere, indtil tokenet udløber.</text>
						</paragraph>
					</body>
				</section>
			</body>
		</section>
	</humanReadableText>
