<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="BuyTransferSellEDaler"
    targetNamespace="https://lab.tagroot.io/Downloads/BuyTransferSellEDaler.xsd"
    elementFormDefault="qualified"
    xmlns="https://lab.tagroot.io/Downloads/BuyTransferSellEDaler.xsd"
    xmlns:mstns="https://lab.tagroot.io/Downloads/BuyTransferSellEDaler.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<!-- Test schema used in example contract. -->
	<!-- Note: Schema is made available via download using the namespace URL. -->

	<xs:element name="BuyEDaler">
		<xs:complexType>
			<xs:attribute name="amount" type="xs:decimal" use="required"/>
			<xs:attribute name="bank" type="xs:string" use="required"/>
			<xs:attribute name="account" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="TransferEDaler">
		<xs:complexType>
			<xs:attribute name="amount" type="xs:decimal" use="required"/>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="SellEDaler">
		<xs:complexType>
			<xs:attribute name="amount" type="xs:decimal" use="required"/>
			<xs:attribute name="bankAccount" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
